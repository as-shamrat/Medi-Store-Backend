{
    "version": 2,
    "headers": [
        {
      "source": "/api/auth/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Credentials", "value": "true" },
        { "key": "Access-Control-Allow-Origin", "value": "https://medistore-client-chi.vercel.app" },
        { "key": "Access-Control-Allow-Methods", "value": "GET,DELETE,PATCH,POST,PUT,OPTIONS" },
        { "key": "Access-Control-Allow-Headers", "value": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version, Authorization, Cookie" }
      ]
    }
  ],
"builds": [
{
"src": "dist/server.js",
"use": "@vercel/node"
}
],
"routes": [
  { "src": "/api/auth/(.*)", "dest": "dist/server.js" },
{
"src": "/(.*)",
"dest": "/dist/server.js"
}
]
}

